#ifndef MCAL_STK_STK_PRV_H_
#define MCAL_STK_STK_PRV_H_

#define STK_AHB       		1
#define STK_AHB_DIV_8       0


/*
 * ENALBE Interrupt Or Disable
 */
#define ENABLE      1
#define DISABLE     0



/*
 * CTRL Pins
 * */

#define CLKSOURCE       2
#define TICKINT       	1
#define CTRL_ENABLE			0
#define COUNTFLAG		16
/*
 *END OF PINS
 * */


#define STK_BASE_ADDR   0xE000E010



typedef struct {

	u32 CTRL;
	u32 LOAD;
	u32 VAL;
	u32 CALIB;

}MSTC_MemMap_t;


#define STK ((volatile MSTC_MemMap_t*)(STK_BASE_ADDR))



#endif /* MCAL_STK_STK_PRV_H_ */
